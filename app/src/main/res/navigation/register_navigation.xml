    android:id="@+id/register_navigation.xml"
    app:startDestination="@id/register_Page1">

    <fragment
        android:id="@+id/register_Page1"
        android:name="com.example.legal_bridge.ui.register.Register_Page1"
        android:label="fragment_register__page1"
        tools:layout="@layout/fragment_register__page1" >
        <action
            android:id="@+id/action_register_Page1_to_register_Page2"
            app:destination="@id/register_Page2" />


    </fragment>
    <fragment
        android:id="@+id/register_Page2"
        android:name="com.example.legal_bridge.ui.register.Register_Page2"
        android:label="fragment_register__page2"
        tools:layout="@layout/fragment_register__page2" >
        <action
            android:id="@+id/action_register_Page2_to_register_Page3"
            app:destination="@id/register_Page3" />
    </fragment>
    <fragment
        android:id="@+id/register_Page3"
        android:name="com.example.legal_bridge.ui.register.Register_Page3"
        android:label="fragment_register__page3"
        tools:layout="@layout/fragment_register__page3" />
</navigation>